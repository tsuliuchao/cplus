[root@VM-10-115-1-18 bin]# ln -s /usr/local/gcc-6.4.0/bin/cpp /usr/bin/cpp
[root@VM-10-115-1-18 bin]# ln -s /usr/local/gcc-6.4.0/bin/gcc /usr/bin/gcc
[root@VM-10-115-1-18 bin]# ln -s /usr/local/gcc-6.4.0/bin/gcov /usr/bin/gcov
[root@VM-10-115-1-18 bin]# ln -s /usr/local/gcc-6.4.0/bin/g++ /usr/bin/g++
